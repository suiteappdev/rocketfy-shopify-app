{
  "family": "shopify-app",
  "networkMode": "awsvpc",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "cpu": 0,
      "memoryReservation": 256,
      "image": "685894002621.dkr.ecr.us-east-1.amazonaws.com/shopify-app:latest",
      "essential": true,
      "name": "shopify-app-container"
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
